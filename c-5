#include<stdio.h>
int main()
{
	int rows,cols,i,j;
	scanf("%d%d",&rows,&cols);
	int arr[rows][cols];
	for(i=0;i<rows;i++)
	{
		for(j=0;j<cols;j++)
		{
			scanf("%d",&arr[i][j]);
		}
	}
	for(i=0;i<rows;i++)
	{
		for(j=0;j<cols;j++)
		{
		//if(i>=j) lower triangle matrix
		if(i<=j) //upper triangle matrix
		printf("%d ",arr[i][j]);
		else
		{
			printf("0 ");
		}
		}
		printf("\n");
	}
	return 0;
}
