#include <stdio.h>

int main() {
    int n, i;
    int first = -1, last = -1;

    scanf("%d", &n);
    int arr[n];

    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int x;
    scanf("%d", &x);
      for(i = 0; i < n; i++) {
        if(arr[i] == x) {
            if(first == -1){
            	first = i;
			}
            last = i;
        }
    }

    printf("%d %d", first, last);

    return 0;
}
