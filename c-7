#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  
    
    int i,arr[n], freq[10] = {0};
    
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] >= 0 && arr[i] <= 9) {  
            freq[arr[i]]++;
        }
    }
    int d;
    for (d = 0; d < 10; d++) {
        if (freq[d] > 0) {
            printf("%d --> %d\n", d, freq[d]);
        }
    }
    return 0;
}
